<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android = "http://schemas.android.com/apk/res/android"
	xmlns:app = "http://schemas.android.com/apk/res-auto"
	android:layout_width = "match_parent"
	android:layout_height = "match_parent"
	android:orientation = "vertical">
	<ImageView android:id = "@+id/logo"
		android:layout_width = "335dp"
		android:layout_height = "119dp"
		app:layout_constraintBottom_toTopOf = "@+id/activity_intro_1_1"
		app:layout_constraintStart_toStartOf = "parent"
		app:layout_constraintEnd_toEndOf = "parent"
		android:layout_marginBottom = "64dp"
		app:srcCompat = "@drawable/ic_logo"/>
	<TextView android:id = "@+id/activity_intro_1_1"
		android:layout_width = "wrap_content"
		android:layout_height = "wrap_content"
		app:layout_constraintTop_toTopOf = "parent"
		app:layout_constraintBottom_toBottomOf = "parent"
		app:layout_constraintStart_toStartOf = "parent"
		app:layout_constraintEnd_toEndOf = "parent"
		android:textSize = "14sp"
		android:text = "@string/activity_intro_1_1"
		android:textColor = "@color/font_color"
		app:layout_constraintVertical_bias = "0.50"/>
	<TextView android:id = "@+id/activity_intro_1_2"
		android:layout_width = "wrap_content"
		android:layout_height = "wrap_content"
		app:layout_constraintTop_toBottomOf = "@+id/activity_intro_1_1"
		app:layout_constraintStart_toStartOf = "@+id/activity_intro_1_1"
		app:layout_constraintEnd_toEndOf = "@+id/activity_intro_1_1"
		android:layout_marginTop = "16dp"
		android:textSize = "14sp"
		android:text = "@string/activity_intro_1_2"
		android:textColor = "@color/font_color"/>
	<ImageView android:id = "@+id/site"
		android:layout_width = "32dp"
		android:layout_height = "32dp"
		app:layout_constraintTop_toTopOf = "@+id/bottom_10"
		app:layout_constraintBottom_toBottomOf = "@+id/bottom_10"
		app:layout_constraintEnd_toStartOf = "@+id/bottom_10"
		android:layout_marginEnd = "16dp"
		app:srcCompat = "@drawable/ic_site"/>
	<ImageView android:id = "@+id/bottom_10"
		android:layout_width = "32dp"
		android:layout_height = "32dp"
		app:layout_constraintTop_toBottomOf = "@+id/activity_intro_1_2"
		app:layout_constraintStart_toStartOf = "parent"
		app:layout_constraintEnd_toEndOf = "parent"
		android:layout_marginTop = "64dp"
		app:srcCompat = "@drawable/ic_telegram"/>
	<ImageView android:id = "@+id/bottom_03"
		android:layout_width = "32dp"
		android:layout_height = "32dp"
		app:layout_constraintTop_toTopOf = "@+id/bottom_10"
		app:layout_constraintBottom_toBottomOf = "@+id/bottom_10"
		app:layout_constraintStart_toEndOf = "@+id/bottom_10"
		android:layout_marginStart = "16dp"
		app:srcCompat = "@drawable/ic_vk"/>
</androidx.constraintlayout.widget.ConstraintLayout>